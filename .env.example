# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_SECRET_TOKEN=your_secret_token_here
TELEGRAM_WEBHOOK_URL=https://your-domain.com/api/telegram/webhook

# API Security
API_SECRET_KEY=your_api_secret_key_here

# Superteam Earn Database Connection (for production)
DATABASE_URL=your_database_url_here

# Environment
NODE_ENV=development

# API URLs
EARN_SUPERTEAM_PUBLIC_API_URL=https://earn.superteam.fun/api

# NGROK_TOKEN : link get from https://dashboard.ngrok.com/get-started/your-authtoken
# This token is used to authenticate your ngrok client
# Make sure to replace 'your_ngrok_token_here' with your actual ngrok token
NGROK_TOKEN=your_ngrok_token_here

# HTTP Proxy (if needed)
HTTP_PROXY=http://your_http_proxy_here
# If you don't need a proxy, you can leave this empty
